dat_smooth = smooth(dat(L, 2), 10);

[dmax, imax] = extrema(dat_smooth);

maxpoint = min(imax);

 

%find first max > datamax

mimax = mimax(mimax > maxpoint);

mimax = min(mimax);

 

%line up maxima

ndat = length(dat(:, 1));

firstpoint = mimax - maxpoint + 1;

lastpoint = mimax - maxpoint + ndat;

model_dat = model_dat(firstpoint : lastpoint);

%start at 0

model_t = model_t - model_t(1);

 

